{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      {% brower => "dom", %}
      "es2015.promise",
      "es5"
    ],
    "module": "ES2015",
    "declaration": true,
    "emitDeclarationOnly": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "skipLibCheck": true
  }
}
